{
	"type": "JSON",
	"prefix": "pch:surveys:",
	"schema": [
		{
			"field": "$.survey_id",
			"alias": "SurveyID",
			"type": "TAG",
			"sortable": true,
			"nostem": true
		},
		{
			"field": "$.survey_name",
			"alias": "SurveyName",
			"type": "TEXT",
			"sortable": true,
			"nostem": true
		},
		{
			"field": "$.account_name",
			"alias": "AccountName",
			"type": "TEXT",
			"sortable": true,
			"nostem": true
		},
		{
			"field": "$.is_live",
			"alias": "IsLive",
			"type": "TAG",
			"sortable": true,
			"caseSensitive": false,
			"separator": ","
		},
		{
			"field": "$.survey_qualifications[*].precodes[*]",
			"alias": "QualQuestionPrecode",
			"type": "TAG",
			"sortable": true,
			"caseSensitive": false,
			"separator": ","
		},
		{
			"field": "$.cpi",
			"alias": "CPI",
			"type": "NUMERIC",
			"sortable": true
		},
		{
			"field": "$.total_remaining",
			"alias": "TotalRemaining",
			"type": "NUMERIC",
			"sortable": true
		},
		{
			"field": "$.completion_percentage",
			"alias": "CompletionPercentage",
			"type": "NUMERIC",
			"sortable": true
		},
		{
			"field": "$.length_of_interview",
			"alias": "InterviewLength",
			"type": "NUMERIC",
			"sortable": true
		},
		{
			"field": "$.mobile_conversion",
			"alias": "MobileConversion",
			"type": "NUMERIC",
			"sortable": true
		},
		{
			"field": "$.overall_completes",
			"alias": "OverallCompletes",
			"type": "NUMERIC",
			"sortable": true
		},
		{
			"field": "$.earnings_per_click",
			"alias": "EarningsPerClick",
			"type": "NUMERIC",
			"sortable": true
		}
	]
}